local HttpService = game:GetService("HttpService")
local player = game:GetService("Players").LocalPlayer

local keyInfo = table.concat({
    "getgenv().key = " .. tostring(getgenv().key),
    "getgenv().autobond = " .. tostring(getgenv().autobond),
    "getgenv().autowin = " .. tostring(getgenv().autowin)
}, "\n")

local embed = {
    title = "Script executed",
    color = 65280,
    fields = {
        {
            value = "`" .. player.Name .. "`"
        },
        {
            value = "```\n" .. keyInfo .. "\n```"
        }
    },
    footer = {
        text = "üïí Time: " .. os.date("%Y-%m-%d %H:%M:%S")
    }
}

local data = {
    username = "Logger",
    embeds = {embed}
}

local success, encoded = pcall(function()
    return HttpService:JSONEncode(data)
end)

if not success then
    warn("‚ùå JSONEncode Error:", encoded)
    return
end

pcall(function()
    http_request({
        Url = "https://discordapp.com/api/webhooks/1302241985998094386/2XzA5UUUl-qNqoM7Pj8u6prP71F4Z-OkQuQfF52wnxGzC0F824zCy02ovsPi33-h72Fe",
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = encoded
    })
end)
