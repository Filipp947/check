local queueteleport = (syn and syn.queue_on_teleport) or queue_on_teleport or (fluxus and fluxus.queue_on_teleport)
if queueteleport then
	queueteleport([[ 
        local CoreGui = game:GetService("CoreGui")
        local RunService = game:GetService("RunService")
        local name = "dead rails @wzaxk"
        local toraScript = CoreGui:FindFirstChild("ToraScript")
        local nameScript = CoreGui:FindFirstChild(name)

        if toraScript then toraScript:Destroy() end
        if nameScript then nameScript:Destroy() end

        RunService.Heartbeat:Connect(function()
            local toraScript = CoreGui:FindFirstChild("ToraScript")
            if toraScript then
                local thirdChild = toraScript:GetChildren()[3]
                if thirdChild then thirdChild:Destroy() end

                toraScript.Name = name

                local imageButton = toraScript:FindFirstChild("ImageButton")
                if imageButton then
                    local frame = imageButton:FindFirstChild("Frame")
                    if frame then
                        local target = frame:GetChildren()[11]
                        if target and target:IsA("TextLabel") then
                            target.Text = "Cracked by bugrovog"
                            target.TextXAlignment = Enum.TextXAlignment.Center
                            target.TextYAlignment = Enum.TextYAlignment.Center
                        end
                    end

                    local targetLabel = toraScript:GetChildren()[2]:FindFirstChild("Frame"):FindFirstChild("TextLabel")
                    if targetLabel and frame then
                        local targetIndex = 12
                        targetLabel.Parent = frame
                        targetLabel.Position = UDim2.new(0, 0, 0, 0)
                        local children = frame:GetChildren()
                        if #children >= targetIndex then
                            targetLabel.Position = children[targetIndex].Position
                        end
                    end
                end

                for _, child in ipairs(toraScript:GetDescendants()) do
                    if child:IsA("TextLabel") then
                        if child.Text == " Gun Aura (Kill Mobs)" then
                            child.Text = "  Gun Aura"
                        elseif child.Text == " Collect Bond & Ammo" then
                            child.Text = "  Collect Bond & Ammo"
                        elseif child.Text == " Items ESP" then
                            child.Text = "  Items ESP"
                        elseif child.Text == " Mobs ESP" then
                            child.Text = "  Mobs ESP"
                        elseif child.Text == " Walk Speed" then
                            child.Text = "  Walk Speed"
                        elseif child.Text == " Noclip" then
                            child.Text = "  Noclip"
                        elseif child.Text == "Dead Rails" then
                            child.Text = "Main"
                        elseif child.Text == "Main" then
                            child.Text = "Usual"
                        end
                    end
                end
            end
        end)

        local function safeLoad()
            local success, result = pcall(function()
                return workspace:WaitForChild("Train", 5):FindFirstChild("Train")
            end)
            if success and result then
                loadstring(game:HttpGet("https://raw.githubusercontent.com/wzaxk/check/refs/heads/main/rails", true))()
            end
        end

        safeLoad()
    ]])
end

local CoreGui = game:GetService("CoreGui")
local RunService = game:GetService("RunService")
local name = "dead rails @wzaxk"
local toraScript = CoreGui:FindFirstChild("ToraScript")
local nameScript = CoreGui:FindFirstChild(name)

if toraScript then toraScript:Destroy() end
if nameScript then nameScript:Destroy() end

RunService.Heartbeat:Connect(function()
	local toraScript = CoreGui:FindFirstChild("ToraScript")
	if toraScript then
		local thirdChild = toraScript:GetChildren()[3]
		if thirdChild then thirdChild:Destroy() end

		toraScript.Name = name

		local imageButton = toraScript:FindFirstChild("ImageButton")
		if imageButton then
			local frame = imageButton:FindFirstChild("Frame")
			if frame then
				local target = frame:GetChildren()[11]
				if target and target:IsA("TextLabel") then
					target.Text = "Cracked by bugrovog"
					target.TextXAlignment = Enum.TextXAlignment.Center
					target.TextYAlignment = Enum.TextYAlignment.Center
				end
			end

			local targetLabel = toraScript:GetChildren()[2]:FindFirstChild("Frame"):FindFirstChild("TextLabel")
			if targetLabel and frame then
				local targetIndex = 12
				targetLabel.Parent = frame
				targetLabel.Position = UDim2.new(0, 0, 0, 0)
				local children = frame:GetChildren()
				if #children >= targetIndex then
					targetLabel.Position = children[targetIndex].Position
				end
			end
		end

		for _, child in ipairs(toraScript:GetDescendants()) do
			if child:IsA("TextLabel") then
				if child.Text == " Gun Aura (Kill Mobs)" then
					child.Text = "  Gun Aura"
				elseif child.Text == " Collect Bond & Ammo" then
					child.Text = "  Collect Bond & Ammo"
				elseif child.Text == " Items ESP" then
					child.Text = "  Items ESP"
				elseif child.Text == " Mobs ESP" then
					child.Text = "  Mobs ESP"
				elseif child.Text == " Walk Speed" then
					child.Text = "  Walk Speed"
				elseif child.Text == " Noclip" then
					child.Text = "  Noclip"
				elseif child.Text == "Dead Rails" then
					child.Text = "Main"
				elseif child.Text == "Main" then
					child.Text = "Usual"
				end
			end
		end
	end
end)

local function safeLoad()
	local success, result = pcall(function()
		return workspace:WaitForChild("Train", 5):FindFirstChild("Train")
	end)
	if success and result then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/wzaxk/check/refs/heads/main/rails", true))()
	end
end

safeLoad()
