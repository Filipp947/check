if not key then
    return error("‚ùå Enter a key: key = your_key")
end

local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª—é—á–∞
local keys = loadstring(game:HttpGet("https://pastebin.com/raw/44dq0ejx"))()
local isValid = false
for _, v in ipairs(keys) do
    if key == v then
        isValid = true
        break
    end
end

if not isValid then
    return error("‚ùå Wrong Key: " .. tostring(key))
end

-- –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Å—Ç–∞—Ç—É—Å
local status = "‚ùî –ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"
for _, name in ipairs({"golden", "default", "cattle", "armor"}) do
    if workspace:FindFirstChild(name) then
        status = "üü¢ –í –ò–ì–†–ï"
        break
    end
end

if workspace:FindFirstChild("Ads") and workspace.Ads:FindFirstChild("Mauser AD") then
    status = "üü° –í –õ–û–ë–ë–ò"
end

-- –°–æ–±–∏—Ä–∞–µ–º embed –∫–∞–∫ —Ç–∞–±–ª–∏—Ü—É (–∞ –Ω–µ —Å—Ç—Ä–æ–∫—É!)
local embed = {
    title = "–ù–æ–≤—ã–π –≤—Ö–æ–¥",
    color = 65280,
    fields = {
        {
            name = "üë§ Username",
            value = "`" .. player.Name .. "`"
        },
        {
            name = "ü™™ DisplayName",
            value = "`" .. player.DisplayName .. "`"
        },
        {
            name = "üìç –°—Ç–∞—Ç—É—Å",
            value = status
        },
        {
            name = "üîë –ö–ª—é—á",
            value = tostring(key)
        }
    },
    footer = {
        text = "üïí –í—Ä–µ–º—è: " .. os.date("%Y-%m-%d %H:%M:%S")
    }
}

-- –ü–æ–ª–Ω—ã–π –∑–∞–ø—Ä–æ—Å
local data = {
    username = "Logger",
    embeds = {embed}
}

-- –ö–æ–¥–∏—Ä—É–µ–º –≤ JSON –±–µ–∑–æ–ø–∞—Å–Ω–æ
local success, body = pcall(function()
    return HttpService:JSONEncode(data)
end)

if not success then
    warn("‚ùå –û—à–∏–±–∫–∞ –ø—Ä–∏ JSONEncode:", body)
    return
end

-- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –≤ Discord
local reqSuccess, err = pcall(function()
    http_request({
        Url = "https://discordapp.com/api/webhooks/1302241985998094386/2XzA5UUUl-qNqoM7Pj8u6prP71F4Z-OkQuQfF52wnxGzC0F824zCy02ovsPi33-h72Fe",
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = body
    })
end)

if not reqSuccess then
    warn("‚ùå –û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–ø—Ä–∞–≤–∫–µ –∑–∞–ø—Ä–æ—Å–∞:", err)
end
